<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog id="ManagementValues_deleteDialog" title="Confirmar Eliminación" draggable="true" resizable="true" type="Message">
    <VBox id="ManagementValues_deleteVBox" class="sapUiSmallMargin">
      <Text id="ManagementValues_deleteConfirmText" text="¿Estás seguro de que deseas eliminar este valor?" />
    </VBox>
    <beginButton>
      <Button id="ManagementValues_btnDeleteConfirm" text="Eliminar" type="Reject" press=".onConfirmDelete"/>
    </beginButton>
    <endButton>
      <Button id="ManagementValues_btnDeleteCancel" text="Cancelar" press=".onCancelDelete"/>
    </endButton>
  </Dialog>
</core:FragmentDefinition>
